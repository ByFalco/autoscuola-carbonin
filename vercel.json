{
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/patenti/patenti-a",
      "destination": "/patenti/patenti-a.html"
    },
    {
      "source": "/patenti/patenti-b",
      "destination": "/patenti/patenti-b.html"
    },
    {
      "source": "/patenti/patenti-superiori",
      "destination": "/patenti/patenti-superiori.html"
    },
    {
